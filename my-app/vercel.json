{
  "buildCommand": "cd my-app && npm run build",
  "outputDirectory": "my-app/.next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/my-app/$1"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "installCommand": "pnpm install --no-frozen-lockfile"
      }
    }
  ]
}